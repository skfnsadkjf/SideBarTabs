{
	"manifest_version": 2,
	"name": "Sidebar Tabs",
	"version": "1.1.3",

	"description": "Makes a vertical sidebar for tabs",

	"sidebar_action": {
		"default_title": "Tabs",
		"default_panel": "sidebar.html"
	},

	"background": {
		"scripts": [
			"background.js"
		]
	},

	"permissions": [
		"tabs",
		"sessions",
		"storage"
	],

	"applications": {
		"gecko": {
			"id": "{c0a99110-9fef-49e7-ab15-f2fb8c83f9d0}"
		}
	}


}